<h1>Editing item</h1>

<% form_for(@item) do |f| %>
  <%= f.error_messages %>
  <table class="edit_win" cellpadding="6" cellspacing="0">
      <%= render :partial => f %>
      <tr>
        <td></td>
        <td><%= f.submit 'Update' %></td>
      </tr>
  </table>
<% end %>

<%= link_to 'Show', @item %> |
<%= link_to 'Back', items_path %>